              },
              "Offer": {
                  "value": "WindowsServer"
              },
              "OSVersion": {
                "value": "[parameters('icaOSVersion')]"
              },              
              "LicenseType": {
                "value": "None"
              },            
              "VMSize": {
                "value": "[parameters('icaVMSize')]"
              },
              "vnetName": {
                "value": "[variables('vnet1Name')]"
              },            
              "subnetName": {
                "value": "[variables('vnet1subnet1Name')]"
              },            
              "vnetprefix": {
                "value": "[variables('vnet1Prefix')]"
              },            
              "subnetPrefix": {
                "value": "[variables('vnet1subnet1Prefix')]"
              },                                                                                                            
              "adminUsername": {
                "value": "[parameters('adminUsername')]"
              },                                                                                                            
              "adminPassword": {
                "value": "[parameters('adminPassword')]"
              },                                                                                                                                        
              "location": {
                  "value": "[parameters('Location1')]"
              }
          }
      }
    },
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2015-01-01",
      "name": "DomainJoinIssuingCA",
      "dependsOn": [
        "VNet1",
        "BastionHost1",
        "DeployDC1VM",
        "promotedc1",
        "updatevnet1dns",
        "restartdc1",
        "configdns",
        "createous",
        "deployrootca",
        "configurerootca",
        "deployissuingca"
      ],      
      "properties": {
          "mode": "Incremental",
          "templateLink": {
              "uri": "[uri(parameters('_artifactsLocation'), concat('nestedtemplates/domainjoin.json', parameters('_artifactsLocationSasToken')))]",
              "contentVersion": "1.0.0.0"
          },
          "parameters": {
              "computerName": {
                  "value": "[variables('icaname')]"
              },
              "domainName": {
                  "value": "[variables('domainName')]"
              },
              "OUPath": {
                  "value": "[variables('SRVOUPath')]"
              },
              "domainJoinOptions": {
                  "value": 3
              },
              "adminUsername": {
                "value": "[parameters('adminUsername')]"
              },                                                                                                            
              "adminPassword": {
                "value": "[parameters('adminPassword')]"
              },                                                                                                                                                                                                                                                                             
              "location": {
                  "value": "[parameters('Location1')]"
              }
          }
      }
    },
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2015-01-01",
      "name": "configureissueca",
      "dependsOn": [
        "VNet1",
        "BastionHost1",
        "DeployDC1VM",
        "promotedc1",
        "updatevnet1dns",
        "restartdc1",
        "configdns",
        "createous",
        "deployrootca",
        "configurerootca",
        "deployissuingca",
        "DomainJoinIssuingCA"
      ],      
      "properties": {
